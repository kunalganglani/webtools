    "protractor": "~5.1.2",
    "pre-commit": "^1.2.2",
    "reflect-metadata": "^0.1.12",
    "ts-loader": "^4.4.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "webpack-cli": "^3.1.0"
  }
